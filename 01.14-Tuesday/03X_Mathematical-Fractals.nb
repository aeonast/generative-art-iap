 RowBox[{
  RowBox[{"naiveJulia", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"zReal_", ",", "zIm_"}], "}"}], ",", "c_", ",", 
    RowBox[{"maxIters_", ":", "100"}], ",", 
    RowBox[{"escapeRadius_", ":", "1"}]}], "]"}], ":=", 
