Cell[BoxData[
 RowBox[{"mnd", "=", 
  RowBox[{"Compile", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"maxiter", ",", "_Integer"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"zinit", ",", "_Complex"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"dt", ",", "_Real"}], "}"}]}], "}"}], ",", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"z", ",", "c", ",", "iters"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"Table", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"z", "=", "zinit"}], ";", "\[IndentingNewLine]", 
         RowBox[{"c", "=", 
          RowBox[{"cr", "+", 
           RowBox[{"I", "*", "ci"}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"iters", "=", "0."}], ";", "\[IndentingNewLine]", 
         RowBox[{"While", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"iters", "<", "maxiter"}], ")"}], "&&", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"Abs", "@", "z"}], "<", "2"}], ")"}]}], ",", 
           RowBox[{
            RowBox[{"iters", "++"}], ";", "\[IndentingNewLine]", 
            RowBox[{"z", "=", 
             RowBox[{
              RowBox[{"z", "^", "2"}], "+", "c"}]}]}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"Sqrt", "[", 
          RowBox[{"iters", "/", "maxiter"}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"cr", ",", 
          RowBox[{"-", "2"}], ",", "2", ",", "dt"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"ci", ",", 
          RowBox[{"-", "2"}], ",", "2", ",", "dt"}], "}"}]}], "]"}]}], "]"}], 
    ",", 
    RowBox[{"CompilationTarget", "\[Rule]", "\"\<C\>\""}], ",", 
